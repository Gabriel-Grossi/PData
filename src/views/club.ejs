<body>
    <div class="sidebar">
        <ul class="logo">
            <li>
                <a href="/dashboard">
                    <img src="assets/logo-xl.svg" alt="Logo">
                </a>
            </li>
        </ul>
        <ul class="list-item">
            <li>
                <a href="/dashboard">
                    <span class="material-icons">
                        home
                    </span>
